<apex:page controller="FileLInk.ExternalFileEditExtn">
    <apex:form >
        <apex:pageBlock title="File Link Edit">
            <apex:pageBlockButtons >
                <apex:commandButton action="{!OnSave}" value="Save"/>
                <apex:commandButton action="{!Cancel}" value="Cancel"/>
            </apex:pageBlockButtons>
            <apex:pageBlockSection columns="1">
                <apex:inputField value="{!externalFileRecord.FileLInk__Is_Private__c}"/>
                <apex:inputField value="{!externalFileRelationRecord.FileLInk__Tags__c}"/>
            </apex:pageBlockSection>
        </apex:pageBlock>
    </apex:form>
</apex:page>